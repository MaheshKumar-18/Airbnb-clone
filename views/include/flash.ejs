
<!-- Success Flash -->
<% if(success && success.length){%>
  <div class="alert alert-success alert-dismissible fade show  col-6 offset-3 popupMsg" role="alert">
  <strong> <%=success%> </strong> 
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
<%} %>

<!-- Error Flash -->
<% if(error && error.length){%>
  <div class="alert alert-danger alert-dismissible fade show  col-6 offset-3 popupMsg" role="alert">
  <strong> <%=error%> </strong> 
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
<%} %>